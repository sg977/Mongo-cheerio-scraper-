<div>
    <h1 class="card z-depth-3 center-align">Updates from ESPN.com in NFL News</h1>
    <div class="row">
      <div class="col s3"></div>
      <h5 class="card z-depth-3 center-align col s6">Click the 'Scrape' button to refresh articles</h5>
    </div>
</div>

<div class="row">
  {{#each article}} 
    <div class ="col s6">
      <div class="card-panel">
          <h4>{{title}}</h4>
          <a href = "/readArticle/{{this._id}}" class="btn blue lighten-1" id="btnSignUp">Article Link</a>
      </div>
    </div>
  {{/each}}
</div>
